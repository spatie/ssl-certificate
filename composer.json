{
    "name": "spatie/ssl-certificate",
    "description": "A class to easily query the properties of an ssl certificate",
    "keywords": [
        "spatie",
        "ssl-certificate"
    ],
    "homepage": "https://github.com/spatie/ssl-certificate",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-intl" : "*",
        "ext-json": "*",
        "nesbot/carbon": "^1.15|^2.0",
        "spatie/macroable": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "spatie/phpunit-snapshot-assertions": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\SslCertificate\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\SslCertificate\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
  }
}
