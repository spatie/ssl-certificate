{
    "name": "spatie/ssl-certificate",
    "description": "A class to easily query the properties of an ssl certificate",
    "keywords": [
        "spatie",
        "ssl-certificate"
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spatie"
        },
        {
            "type": "other",
            "url": "https://spatie.be/open-source/support-us"
        }
    ],
    "homepage": "https://github.com/spatie/ssl-certificate",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-intl": "*",
        "ext-json": "*",
        "nesbot/carbon": "^2.68.1",
        "spatie/macroable": "^2.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.12.2",
        "spatie/pest-plugin-snapshots": "^2.0.1",
        "spatie/ray": "^1.37.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\SslCertificate\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\SslCertificate\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
